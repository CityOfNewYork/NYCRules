INSERT INTO `field_revision_body` VALUES ('node','page','0','15','17','und','0','<p><span class="area_header"><strong>About NYC Rules</strong></span></p><p><br />\r\n<span class="bodytext">Rulemaking represents one of the most basic interactions between the public and City government. For example, in which City parks can you gather with family and friends for a barbecue?&nbsp; How do you obtain a license for your dog?&nbsp; When are you required to shovel the snow from the sidewalk in front of your property?<br />\r\n<br /> The answers to questions like these are found in agency rules.&nbsp; City agencies enact rules through a legal process that generally provides the public with an opportunity to comment on proposed rules before they become law. Because these rules affect New Yorkers in so many ways, Mayor Michael R. Bloomberg created NYC Rules to enhance the public''s access to the rulemaking process, and to invite members of the public to comment on proposed rules online, in addition to traditional methods such as submission by mail directly to the rulemaking agency or testimony at a public hearing.<br /> <br /></span> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="http://www.nyc.gov/html/nycrules/html/rulemaking/rulemaking.shtml">Go to Rulemaking Process Map</a><br /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="http://www.nyc.gov/html/nycrules/html/about/process.shtml">Go to Rulemaking Process 101</a><br /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="http://www.nyc.gov/html/nycrules/html/about/eo133.shtml">Go to Executive Order 133</a><br /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="http://www.nyc.gov/html/nycrules/html/capa/capa.shtml">Go to City Administrative Procedure Act</a><br /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="http://www.nycgovparks.org/facilities/barbecue">Find out which City parks have barbecue areas</a><br /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="https://a816-healthpsi.nyc.gov/DogLicense/Login.do">Apply for a new dog license, or renew an existing dog license</a><br /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/arrow.gif" alt="" border="0" /><a class="bodytext_underline" href="http://www.nyc.gov/html/dsny/html/about/column_feb08.shtml">Find out more about the rules regarding snow removal</a></p>','','full_html');
INSERT INTO `field_revision_body` VALUES ('node','page','0','16','18','und','0','<p><span class="area_header">Rulemaking Process Map<br /></span> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/spacers/spacer_30_30.gif" alt="" width="30" height="30" border="0" /> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/spacers/spacer_499_15.gif" alt="" width="499" height="15" border="0" /><br />  <span class="bodytext"></span></p><table width="499" border="0" cellspacing="0" cellpadding="0"><tbody><tr><td><table class="blue" width="499" border="0" cellspacing="0" cellpadding="0"><tbody><tr><td class="header">Notice of proposed rule sent to the City Record, City Council, Community Boards, media outlets, and civic organizations</td></tr><tr><td class="rulemaking">Notice must include:<ul class="no-indent"><li>legal authority and purpose of proposed rule</li><li>text of proposed rule</li><li>time and place of public hearing to discuss proposed rule</li><li>address for written comments, if not submitted through NYC Rules</li></ul></td></tr></tbody></table></td></tr><tr><td align="center"><img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/rulemaking_arrow.gif" alt="" width="41" height="42" border="0" /></td></tr><tr><td><table class="green" width="499" border="0" cellspacing="0" cellpadding="0"><tbody><tr><td class="header">Notice of the public hearing published in City Record</td></tr><tr><td class="rulemaking">Must be published at least 30 days prior to public hearing<br /> <br /> Will also be posted on NYC Rules Web site</td></tr></tbody></table></td></tr><tr><td align="center"><img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/rulemaking_arrow.gif" alt="" width="41" height="42" border="0" /></td></tr><tr><td><table class="yellow" width="499" border="0" cellspacing="0" cellpadding="0"><tbody><tr><td class="header">Public hearing to discuss the proposed rule</td></tr><tr><td class="rulemaking">Rulemaking agency must accept from the public and from other agencies, and enter into the record:<ul class="no-indent"><li>comments given at the public hearing</li><li>written comments submitted within the comment period to rulemaking agency, to NYC Rules, or both</li></ul></td></tr></tbody></table></td></tr><tr><td align="center"><img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/rulemaking_arrow.gif" alt="" width="41" height="42" border="0" /></td></tr><tr><td><table class="orange" width="499" border="0" cellspacing="0" cellpadding="0"><tbody><tr><td class="header">Final Rule published in City Record and submitted to City Council</td></tr><tr><td class="rulemaking">Rulemaking agency may make changes to proposed rule that reflect comments entered into record</td></tr></tbody></table></td></tr><tr><td align="center"><img src="http://www.nyc.gov/html/nycrules/includes/site_images/misc/rulemaking_arrow.gif" alt="" width="41" height="42" border="0" /></td></tr><tr><td><table class="black" width="499" border="0" cellspacing="0" cellpadding="0"><tbody><tr><td class="header">Rule takes effect 30 days after publication in City Record</td></tr></tbody></table></td></tr></tbody></table><p><span class="bodytext"></span> <img src="http://www.nyc.gov/html/nycrules/includes/site_images/spacers/spacer_60_30.gif" alt="" width="60" height="30" border="0" /></p>','','full_html');
